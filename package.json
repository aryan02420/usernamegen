{
  "name": "@aryan02420/usernamegen",
  "version": "1.0.1",
  "description": "generates a random username",
  "keywords": [
    "username",
    "random",
    "generator",
    "seed"
  ],
  "homepage": "https://usernamegen.js.org",
  "author": "aryan02420 <aryan02420@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/",
    "bin/"
  ],
  "scripts": {
    "test": "mocha --require mocha-steps",
    "build": "tsc",
    "build:browser": "browserify --standalone UNG ./lib/index.js -o ./lib/browser.js && terser ./lib/browser.js -c -o ./lib/browser.min.js",
    "all": "npm run build && npm run build:browser && npm run test"
  },
  "bin": {
    "ung": "bin/ung.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryan02420/usernamegen.git"
  },
  "dependencies": {
    "seedrandom": "^3.0.5"
  },
  "bugs": {
    "url": "https://github.com/aryan02420/usernamegen/issues"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.3.2",
    "mocha-steps": "^1.3.0",
    "terser": "^5.6.1",
    "typescript": "^4.2.3"
  }
}
